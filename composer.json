{
    "name": "webignition/app.simplytestable.com",
    "description": "Source for http://app.simplytestable.com",
    "require": {
        "php": ">=7.2.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "chrisboulton/php-resque": "dev-master",
        "chrisboulton/php-resque-scheduler": "dev-master",
        "cron/cron-bundle": "1.2.3",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^1",
        "doctrine/migrations": "~1.8",
        "doctrine/orm": "^2.6",
        "egulias/email-validator": "^2",
        "friendsofsymfony/user-bundle": "2.1.2",
        "guzzlehttp/guzzle": "~6",
        "resquebundle/resque": "^2",
        "sensio/framework-extra-bundle": "^5.0.0",
        "stripe/stripe-php": "5.8.0",
        "symfony/console": "^4.1",
        "symfony/dotenv": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.1",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/security-bundle": "^4.1",
        "symfony/yaml": "^4.1",
        "twig/twig": "^2.0",
        "webignition/guzzle-curl-exception": "~1",
        "webignition/guzzle-http-authentication-middleware": ">=0.1,<1",
        "webignition/guzzle-request-headers-middleware": ">=0.1,<1",
        "webignition/html-document-link-finder": "^3.1",
        "webignition/html-validation-error-normaliser": "^1",
        "webignition/http-history-container": ">=0.5,<1",
        "webignition/internet-media-type": ">=0.4.4,<2.0",
        "webignition/php-stripe-models": ">=0.2.6,<1.0",
        "webignition/sitemap-model": "~3",
        "webignition/url": "~2",
        "webignition/url-resolver": "~2",
        "webignition/web-json-model": "~3",
        "webignition/web-page-model": "~3",
        "webignition/web-resource-model": "~3",
        "webignition/web-resource-retriever": ">=0.7,<1",
        "webignition/website-rss-feed-finder": "~1",
        "webignition/website-sitemap-finder": "~2",
        "wildbit/postmark-php": "^2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.1",
        "phpunit/phpunit": "~7.0",
        "squizlabs/php_codesniffer": "3.*",
        "mockery/mockery": "^1.0",
        "php-mock/php-mock-mockery": "^1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "AppBundle\\": "src/AppBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "Tests\\AppBundle\\": "tests/AppBundle"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test-unit": "./vendor/bin/phpunit --colors tests/AppBundle/Unit/",
        "test-functional": "./vendor/bin/phpunit --colors tests/AppBundle/Functional/",
        "test-unit-stop": "./vendor/bin/phpunit --colors --stop-on-error --stop-on-failure tests/AppBundle/Unit/",
        "test-functional-stop": "./vendor/bin/phpunit --colors --stop-on-error --stop-on-failure tests/AppBundle/Functional/",
        "test": [
            "@composer test-unit",
            "@composer test-functional"
        ],
        "test-stop": [
            "@composer test-unit-stop",
            "@composer test-functional-stop"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
