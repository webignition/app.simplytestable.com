    {
    "name": "webignition/app.simplytestable.com",
    "description": "Source for http://app.simplytestable.com",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" },
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": ">=5.6.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "incenteev/composer-parameter-handler": "^2.0",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.0.0",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/symfony": "3.4.*",
        "twig/twig": "^1.0||^2.0",
        "friendsofsymfony/user-bundle": "2.1.2",
        "doctrine/migrations": "~1.5",
        "doctrine/doctrine-migrations-bundle": "1.3.1",
        "doctrine/doctrine-fixtures-bundle": "~2.3",
        "stephenfarrell/postmark-bundle": "dev-master#09eb492d79e823ee256f75b2d2f16df01dc5d146",
        "webignition/internet-media-type":">=0.4.4,<2.0",
        "webignition/website-sitemap-finder":"~2",
        "webignition/url": "~2",
        "webignition/website-rss-feed-finder":"~1",
        "guzzlehttp/guzzle":"~6",
        "jasonlewis/expressive-date": "1.0.*",
        "stripe/stripe-php": "3.0.0",
        "webignition/html-validation-error-normaliser":"dev-master",
        "webignition/html-document-link-finder":">=0.7,<1.0",
        "webignition/url-resolver":"~2",
        "webignition/php-stripe-models":">=0.2.6,<1.0",
        "egulias/email-validator":"1.1.1",
        "webignition/web-resource-retriever":">=0.7,<1",
        "chrisboulton/php-resque": "dev-master",
        "chrisboulton/php-resque-scheduler": "dev-master",
        "resquebundle/resque": "1.2.0",
        "webignition/web-resource-model": "~3",
        "webignition/web-json-model": "~3",
        "webignition/web-page-model": "~3",
        "cron/cron-bundle": "1.2.3",
        "webignition/sitemap-model":"~3",
        "webignition/guzzle-curl-exception":"~1",
        "webignition/resque-job-factory": ">=0.1.1,<1.0",
        "webignition/guzzle-http-authentication-middleware": ">=0.1,<1",
        "webignition/http-history-container": ">=0.5,<1",
        "webignition/guzzle-request-headers-middleware": ">=0.1,<1"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "phpunit/phpunit": "~5.0",
        "squizlabs/php_codesniffer": "3.*",
        "mockery/mockery": "^1.0",
        "php-mock/php-mock-mockery": "1.2.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ],
        "test-unit": "./vendor/bin/phpunit --colors tests/ApiBundle/Unit/",
        "test-functional": "./vendor/bin/phpunit --colors tests/ApiBundle/Functional/",
        "test": [
            "@composer test-unit",
            "@composer test-functional"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable":true,
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-tests-dir": "tests",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    }
}
