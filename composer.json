{
    "name": "webignition/app.simplytestable.com",
    "description": "Source for http://app.simplytestable.com",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.4.0",
        "symfony/symfony": "2.5.*",
        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~3.0,>=3.0.12",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "friendsofsymfony/user-bundle": "1.3.2",
        "jms/serializer-bundle": "0.9",
        "doctrine/migrations": "~1.5",
        "doctrine/doctrine-migrations-bundle": "~1.2",
        "doctrine/doctrine-fixtures-bundle": "~2.3",
        "winzou/console-bundle":"1.0.0",
        "mlpz/postmark-bundle": "*",
        "webignition/internet-media-type":">=0.4.4,<2.0",
        "webignition/website-sitemap-finder":">=1.0.2,<2.0",
        "webignition/website-sitemap-retriever":">=1.0.1,<2.0",
        "webignition/url": ">=1.9.12",
        "simplepie/simplepie":">=1.3.1,<1.4",
        "webignition/website-rss-feed-finder":">=0.2.1,<1.0",
        "guzzle/guzzle":">=3.8.0,<4",
        "jasonlewis/expressive-date": "1.0.*",
        "stripe/stripe-php": "1.7.15",
        "webignition/html-validation-error-normaliser":"dev-master",
        "webignition/html-document-link-finder":">=0.7,<1.0",
        "webignition/url-resolver":">=0.3,<1.0",
        "webignition/php-stripe-models":">=0.2.5,<1.0",
        "webignition/cookie-url-matcher":">=0.1,<1",
        "egulias/email-validator":"1.1.1",
        "webignition/web-resource-service":"1.0.3",
        "chrisboulton/php-resque": "dev-master",
        "chrisboulton/php-resque-scheduler": "dev-master",
        "bcc/resque-bundle": "dev-master",
        "webignition/web-resource-model": "1.1.8",
        "webignition/web-json-model": "1.0.1",
        "webignition/web-page-model": "1.0.2",
        "cron/cron-bundle": "1.0.*"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "icanboogie/inflector":">=1.0,<2",
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "3.*",
        "mockery/mockery": "~0.9"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "test-unit": "./bin/phpunit -c app --colors src/SimplyTestable/ApiBundle/Tests/Unit/",
        "test-functional": "./bin/phpunit -c app --colors src/SimplyTestable/ApiBundle/Tests/Functional/",
        "test": [
            "@composer test-unit",
            "@composer test-functional"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "prefer-stable":true,
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/webignition/ResqueBundle"
        }
    ]
}
